{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "default-case": 0,
    "dot-notation": 0,
    "comma-dangle": 0,
    "function-paren-newline": 0,
    "guard-for-in": 0,
    "import/prefer-default-export": 0,
    "indent": 0,
    "no-case-declarations": 0,
    "object-curly-newline": 0,
    "object-curly-spacing": 0,
    "object-shorthand": 0,
    "no-console": 0,
    "no-confusing-arrow": 0,
    "prop-types": 0,
    "react/display-name": 0,
    "max-len": [
      1,
      135,
      2,
      {
        "ignoreComments": true
      }
    ]
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "src"
        ]
      }
    }
  }
}